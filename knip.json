{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/main.tsx", "src/vite-env.d.ts", "scripts/**/*.{ts,mts,mjs}", "vitest.config.ts"],
  "project": [
    "src/**/*.{ts,tsx}",
    "!src/**/__tests__/**",
    "!src/**/*.test.{ts,tsx}",
    "!src/**/*.spec.{ts,tsx}",
    "scripts/**/*.{ts,mts,mjs}"
  ],
  "ignore": [
    ".github/workflows/**",
    "src/test/**",
    "src/**/__tests__/**",
    "src/**/*.test.{ts,tsx}",
    "src/**/*.spec.{ts,tsx}",
    "coverage/**",
    "vite.config.*",
    "dist/**",
    "documentation/**",
    "audits/**",
    "src/types/web-component-events.ts",
    "src/types/ui.ts",
    "src/types/api.ts",
    "src/types/components.ts",
    "src/types/dashboard.ts",
    "src/types/utils.ts",
    "src/types/stock.ts",
    "src/constants/stockConfig.ts",
    "src/utils/aiPredictions.ts",
    "src/utils/unitFormatter.ts",
    "src/utils/containerManager.ts"
  ],
  "ignoreDependencies": ["lighthouse", "@testing-library/user-event"],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false,
  "ignoreBinaries": ["powershell"],
  "vitest": {
    "entry": ["src/**/__tests__/**/*.test.{ts,tsx}", "src/**/*.test.{ts,tsx}"]
  },
  "workspaces": {
    ".": {
      "entry": "src/main.tsx"
    }
  }
}
