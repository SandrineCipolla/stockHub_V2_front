{
  "name": "stockhub-v2-front",
  "private": true,
  "version": "1.7.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File ./kill-vite.ps1 && npm run dev",
    "kill-vite": "powershell -ExecutionPolicy Bypass -File ./kill-vite.ps1",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "build:with-sitemap": "npm run generate-sitemap && npm run build",
    "generate-sitemap": "tsx scripts/generate-sitemap.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "detect:as-const": "node scripts/detect-as-const.mjs",
    "clean:deadcode": "knip --config knip.json",
    "clean:fix": "knip --fix",
    "clean:check": "npm run type-check && npm run lint && npm run clean:deadcode",
    "check:strict": "npm run detect:as-const && npm run lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "audit:fps": "node scripts/audit-fps.mjs",
    "audit:a11y": "node scripts/audit-a11y.mjs",
    "audit:datasets": "node scripts/audit-datasets.mjs",
    "audit:colorblind": "node scripts/audit-colorblind.mjs",
    "audit:wcag": "node scripts/audit-wcag.mjs",
    "audit:full": "node scripts/audit-full.mjs",
    "generate:a11y": "node scripts/generate-a11y.mjs",
    "generate:wcag": "node scripts/generate-wcag.mjs",
    "generate:eco": "node scripts/generate-eco.mjs",
    "generate:coverage": "node scripts/generate-coverage.mjs",
    "generate:lighthouse": "node scripts/generate-lighthouse.mjs",
    "generate:all": "node scripts/generate-all.mjs",
    "merge:audits": "node scripts/merge-audits.mjs",
    "metrics:update": "node scripts/update-metrics-files.mjs",
    "metrics:dashboard": "npm run generate:all && npm run metrics:update",
    "metrics:serve": "node scripts/serve-metrics.mjs",
    "ci:quality": "npm run type-check && npm run lint && npm run clean:deadcode && npm run detect:as-const",
    "ci:test": "npm run test:run",
    "ci:build": "npm run build",
    "ci:check": "npm run ci:quality && npm run ci:test && npm run ci:build",
    "ci:fix": "npm run lint:fix && npm run clean:fix && npm run ci:check",
    "ci:fast": "npm run type-check && npm run lint && npm run test:run",
    "ci:full": "npm run ci:check && npm run audit:full",
    "debug:ci": "node scripts/debug-ci.mjs",
    "prepare": "husky",
    "pre-commit": "npm run clean:check"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.27.0",
    "@azure/msal-react": "^3.0.23",
    "@stockhub/design-system": "github:SandrineCipolla/stockhub_design_system#v1.3.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.517.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "^7.13.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "knip": "^5.66.2",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.38",
    "prettier": "^3.7.4",
    "puppeteer": "^24.24.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
