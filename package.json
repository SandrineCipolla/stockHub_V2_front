{
  "name": "stockhub-v2-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate-sitemap": "tsx scripts/generate-sitemap.ts",
    "build": "npm run generate-sitemap && tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "audit:fps": "node scripts/test-performance-fps.mjs",
    "audit:a11y": "node scripts/test-reduced-motion.mjs",
    "audit:datasets": "node scripts/test-animations-datasets.mjs",
    "audit:daltonisme": "node scripts/test-daltonisme.mjs",
    "audit:full": "node scripts/audit-complet.mjs",
    "clean:deadcode": "knip",
    "clean:fix": "knip --fix",
    "clean:check": "npm run type-check && npm run lint && npm run clean:deadcode",
    "pre-commit": "npm run clean:check",
    "detect:as-const": "node scripts/detect-as-const.mjs",
    "check:strict": "npm run detect:as-const && npm run lint"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.517.0",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "stockhub_design_system": "github:SandrineCipolla/stockhub_design_system"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^27.0.0",
    "knip": "^5.66.2",
    "postcss": "^8.4.38",
    "puppeteer": "^24.24.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
